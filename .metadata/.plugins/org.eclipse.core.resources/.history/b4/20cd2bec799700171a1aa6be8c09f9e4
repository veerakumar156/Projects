import java.util.ArrayList;
import java.util.Collections;

/**
 * 
 */


/**
 * @author Veerakumar
 *
 */
public class ProcessLinkedList {


	public static void main(String[] args) {
		LinkedList list = new LinkedList(); 
		int[] arr = {1, 5, 2, 13, 7, 3};
		List<int> arrList = new ArrayList(int);
		arrList.add(arr);
		Collections.sort(arrList, (a, b) -> b.compareTo(a));
		for (int i: arr)
			list.addNode(i); 
		
		list.printList();
	}

}
